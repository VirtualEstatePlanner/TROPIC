{
  "extends": ["eslint: recommended", "plugin: import/errors", "prettier"],
  "rules": {
    "no-console": true
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "root": true
}
